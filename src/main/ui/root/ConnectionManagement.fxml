<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity"
  minHeight="-Infinity" minWidth="-Infinity" prefHeight="250.0" prefWidth="300.0" spacing="20.0"
  xmlns="http://javafx.com/javafx/8.0.111"
  fx:controller="main.ui.root.ConnectionManagementController">
  <children>
    <HBox>
      <children>
        <StackPane>
          <children>
            <Label text="Roomba"/>
          </children>
        </StackPane>
        <Region HBox.hgrow="ALWAYS"/>
        <JFXComboBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
          minWidth="-Infinity" prefWidth="160.0" promptText="Serial Devices"/>
      </children>
    </HBox>
    <HBox>
      <children>
        <StackPane>
          <children>
            <Label text="Start Mode"/>
          </children>
        </StackPane>
        <Region HBox.hgrow="ALWAYS"/>
        <JFXComboBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
          minWidth="-Infinity" prefWidth="160.0" promptText="Modes"/>
      </children>
    </HBox>
    <StackPane VBox.vgrow="ALWAYS" alignment="BOTTOM_LEFT">
      <children>
        <JFXButton text="Connect"/>
      </children>
    </StackPane>
  </children>
  <padding>
    <Insets bottom="10.0" left="10.0" right="10.0" top="20.0"/>
  </padding>
</VBox>
