<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity"
  maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" prefHeight="30.0"
  stylesheets="@root.css" xmlns="http://javafx.com/javafx/8.0.111"
  fx:controller="main.ui.root.ControllerNavigationController" fx:id="controllerNavigation">
  <left>
    <HBox BorderPane.alignment="CENTER_LEFT" alignment="CENTER_LEFT">
      <children>
        <JFXButton fx:id="leftButton"/>
        <Label text="Label" fx:id="leftLabel"/>
      </children>
      <BorderPane.margin>
        <Insets
          bottom="10.0"/> <!--if ControllerNavigation is moved above modules change bottom to top-->
      </BorderPane.margin>
    </HBox>
  </left>
  <right>
    <HBox BorderPane.alignment="CENTER_RIGHT" alignment="CENTER_RIGHT">
      <children>
        <Label text="Label" fx:id="rightLabel"/>
        <JFXButton fx:id="rightButton"/>
      </children>
      <BorderPane.margin>
        <Insets
          bottom="10.0"/> <!--if ControllerNavigation is moved above modules change bottom to top-->
      </BorderPane.margin>
    </HBox>
  </right>
</BorderPane>
