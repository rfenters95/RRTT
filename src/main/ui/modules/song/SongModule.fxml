<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXComboBox?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import main.ui.modules.song.note.NoteControl?>
<ScrollPane xmlns:fx="http://javafx.com/fxml/1" fitToWidth="true" id="song-module"
  maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
  prefHeight="250.0" prefWidth="800.0" stylesheets="@../../modules/main.css"
  xmlns="http://javafx.com/javafx/8.0.111" fx:controller="main.ui.modules.song.SongModuleController"
  fx:id="songModule">
  <content>
    <VBox spacing="25.0">
      <children>
        <StackPane>
          <children>
            <GridPane hgap="25" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="575.0"
              styleClass="song-module-gridPane" vgap="10">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
              </rowConstraints>
              <children>
                <BorderPane>
                  <left>
                    <Label BorderPane.alignment="CENTER" text="Song Number"/>
                  </left>
                  <right>
                    <JFXComboBox BorderPane.alignment="CENTER" prefWidth="125"
                      fx:id="songNumberCB"/>
                  </right>
                </BorderPane>
                <BorderPane GridPane.columnIndex="1">
                  <left>
                    <Label BorderPane.alignment="CENTER" text="Song Length"/>
                  </left>
                  <right>
                    <JFXComboBox BorderPane.alignment="CENTER" prefWidth="125"
                      fx:id="songLengthCB"/>
                  </right>
                </BorderPane>
                <BorderPane GridPane.rowIndex="1">
                  <left>
                    <Label BorderPane.alignment="CENTER" text="Output Destination"/>
                  </left>
                  <right>
                    <JFXComboBox BorderPane.alignment="CENTER" prefWidth="125" fx:id="outputCB"/>
                  </right>
                </BorderPane>
                <BorderPane GridPane.columnIndex="1" GridPane.rowIndex="1">
                  <left>
                    <Label BorderPane.alignment="CENTER" text="Sleep Time"/>
                  </left>
                  <right>
                    <JFXComboBox BorderPane.alignment="CENTER" prefWidth="125" fx:id="sleepCB"/>
                  </right>
                </BorderPane>
              </children>
              <padding>
                <Insets bottom="20" left="10" right="10" top="10"/>
              </padding>
            </GridPane>
          </children>
        </StackPane>
        <StackPane>
          <children>
            <GridPane hgap="15" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="775.0"
              styleClass="song-module-gridPane" vgap="10">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES"/>
              </rowConstraints>
              <children>

                <NoteControl GridPane.columnIndex="0" GridPane.rowIndex="0" text="Note 1"
                  fx:id="noteControl1"/>
                <NoteControl GridPane.columnIndex="1" GridPane.rowIndex="0" text="Note 2"
                  fx:id="noteControl2"/>
                <NoteControl GridPane.columnIndex="0" GridPane.rowIndex="1" text="Note 3"
                  fx:id="noteControl3"/>
                <NoteControl GridPane.columnIndex="1" GridPane.rowIndex="1" text="Note 4"
                  fx:id="noteControl4"/>

                <NoteControl GridPane.columnIndex="0" GridPane.rowIndex="2" text="Note 5"
                  fx:id="noteControl5"/>
                <NoteControl GridPane.columnIndex="1" GridPane.rowIndex="2" text="Note 6"
                  fx:id="noteControl6"/>
                <NoteControl GridPane.columnIndex="0" GridPane.rowIndex="3" text="Note 7"
                  fx:id="noteControl7"/>
                <NoteControl GridPane.columnIndex="1" GridPane.rowIndex="3" text="Note 8"
                  fx:id="noteControl8"/>

                <NoteControl GridPane.columnIndex="0" GridPane.rowIndex="4" text="Note 9"
                  fx:id="noteControl9"/>
                <NoteControl GridPane.columnIndex="1" GridPane.rowIndex="4" text="Note 10"
                  fx:id="noteControl10"/>
                <NoteControl GridPane.columnIndex="0" GridPane.rowIndex="5" text="Note 11"
                  fx:id="noteControl11"/>
                <NoteControl GridPane.columnIndex="1" GridPane.rowIndex="5" text="Note 12"
                  fx:id="noteControl12"/>

                <NoteControl GridPane.columnIndex="0" GridPane.rowIndex="6" text="Note 13"
                  fx:id="noteControl13"/>
                <NoteControl GridPane.columnIndex="1" GridPane.rowIndex="6" text="Note 14"
                  fx:id="noteControl14"/>
                <NoteControl GridPane.columnIndex="0" GridPane.rowIndex="7" text="Note 15"
                  fx:id="noteControl15"/>
                <NoteControl GridPane.columnIndex="1" GridPane.rowIndex="7" text="Note 16"
                  fx:id="noteControl16"/>

              </children>
            </GridPane>
          </children>
        </StackPane>
      </children>
      <padding>
        <Insets bottom="20.0" top="20.0"/>
        <!--<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />-->
      </padding>
    </VBox>
  </content>
</ScrollPane>
