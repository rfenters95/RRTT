<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER" maxHeight="-Infinity"
  maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" spacing="15.0" type="HBox"
  xmlns="http://javafx.com/javafx/8.0.111">

  <children>

    <Label maxWidth="-Infinity" minWidth="-Infinity" prefWidth="50.0" text="Note"
      fx:id="noteLabel"/>
    <JFXComboBox maxWidth="-Infinity" minWidth="-Infinity" prefWidth="65.0" promptText="Pitch"
      fx:id="pitchComboBox"/>
    <JFXComboBox HBox.hgrow="ALWAYS" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="130.0"
      promptText="Duration" fx:id="durationComboBox"/>
    <JFXButton contentDisplay="CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
      minHeight="-Infinity" minWidth="-Infinity" onAction="#play" prefHeight="30" prefWidth="30"
      styleClass="play-button" text=" " fx:id="playButton"/>

  </children>
  <padding>
    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
  </padding>
</fx:root>
