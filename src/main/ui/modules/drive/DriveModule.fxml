<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity"
  minHeight="-Infinity" minWidth="-Infinity" prefHeight="250.0" prefWidth="250.0" spacing="20.0"
  stylesheets="@../../modules/main.css" xmlns="http://javafx.com/javafx/8.0.111"
  fx:controller="main.ui.modules.drive.DriveModuleController" fx:id="driveModule">
  <children>
    <JFXComboBox labelFloat="true" promptText="Drive Mode" fx:id="driveModeComboBox"/>
    <VBox prefHeight="200.0" prefWidth="100.0" spacing="18.0">
      <children>
        <JFXTextField labelFloat="true" maxHeight="-Infinity" maxWidth="-Infinity"
          minHeight="-Infinity" minWidth="-Infinity" promptText="Input 1" fx:id="textField1"/>
        <JFXTextField labelFloat="true" maxHeight="-Infinity" maxWidth="-Infinity"
          minHeight="-Infinity" minWidth="-Infinity" promptText="Input 2" fx:id="textField2"/>
      </children>
    </VBox>
    <StackPane alignment="BOTTOM_LEFT">
      <children>
        <JFXButton onAction="#forward" text="Start"/>
      </children>
    </StackPane>
  </children>
  <padding>
    <Insets bottom="10.0" left="10.0" right="10.0" top="20.0"/>
  </padding>
</VBox>
