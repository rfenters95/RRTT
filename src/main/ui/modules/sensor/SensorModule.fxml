<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity"
  minHeight="-Infinity" minWidth="-Infinity" prefHeight="250.0" prefWidth="250.0"
  stylesheets="@../../modules/main.css" xmlns="http://javafx.com/javafx/8.0.111"
  fx:controller="main.ui.modules.sensor.SensorModuleController" fx:id="sensorModule" styleClass="module">
  <children>
    <StackPane VBox.vgrow="ALWAYS" alignment="TOP_LEFT" maxWidth="1.7976931348623157E308"
      minWidth="-Infinity">
      <children>
        <VBox maxHeight="-Infinity" maxWidth="1.7976931348623157E308" minHeight="-Infinity"
          minWidth="-Infinity" spacing="26.0">
          <children>
            <JFXComboBox labelFloat="true" maxHeight="-Infinity" maxWidth="1.7976931348623157E308"
              minHeight="-Infinity" minWidth="-Infinity" prefHeight="30.0" prefWidth="180.0"
              promptText="Boolean Sensors" fx:id="booleanSensorComboBox"/>
            <JFXButton contentDisplay="CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
              styleClass="play-button"
              minHeight="-Infinity" minWidth="-Infinity" onAction="#readBooleanSensor"
              prefHeight="30.0" prefWidth="30.0" text=" " fx:id="toggleBooleanSensorButton">
              <graphic>
                <ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true"
                  preserveRatio="true">
                  <image>
                    <Image url="@../../../res/play.png"/>
                  </image>
                </ImageView>
              </graphic>
            </JFXButton>
          </children>
        </VBox>
      </children>
      <VBox.margin>
        <Insets/>
      </VBox.margin>
    </StackPane>
    <StackPane VBox.vgrow="ALWAYS" alignment="BOTTOM_LEFT" maxWidth="1.7976931348623157E308"
      minWidth="-Infinity">
      <children>
        <VBox maxHeight="-Infinity" maxWidth="1.7976931348623157E308" minHeight="-Infinity"
          minWidth="-Infinity" spacing="26.0">
          <children>
            <JFXComboBox labelFloat="true" maxHeight="-Infinity" maxWidth="1.7976931348623157E308"
              minHeight="-Infinity" minWidth="-Infinity" prefHeight="30.0" prefWidth="180.0"
              promptText="Signal Sensors" fx:id="signalSensorComboBox"/>
            <JFXButton contentDisplay="CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
              styleClass="play-button"
              minHeight="-Infinity" minWidth="-Infinity" onAction="#readSignalSensor"
              prefHeight="30.0" prefWidth="30.0" text=" " fx:id="toggleSignalSensorButton">
              <graphic>
                <ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true"
                  preserveRatio="true">
                  <image>
                    <Image url="@../../../res/play.png"/>
                  </image>
                </ImageView>
              </graphic>
            </JFXButton>
          </children>
        </VBox>
      </children>
      <VBox.margin>
        <Insets/>
      </VBox.margin>
    </StackPane>
  </children>
  <padding>
    <Insets bottom="10.0" left="20.0" right="20.0" top="20.0"/>
  </padding>
</VBox>
